{
  "name": "kjs",
  "version": "1.0.3",
  "description": "ES6 Universal Library Starter",
  "main": "./index.js",
  "scripts": {
    "prepublish": "npm run build-all",
    "preversion": "npm run build-all && npm run unit",
    "version": "git add .",
    "postversion": "git push && git push --tags",
    "dev": "gulp build-web-dev",
    "build": "gulp build",
    "build-dev": "gulp build-dev",
    "build-web": "gulp build-web",
    "build-web-dev": "gulp build-web-dev",
    "build-all": "gulp",
    "unit": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "unit-watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
    "test": "npm run unit-watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liady/es6-library-minimal.git"
  },
  "keywords": [
    "es6",
    "npm",
    "starter",
    "library",
    "boilerplate",
    "babel",
    "umd",
    "webpack",
    "universal"
  ],
  "author": {
    "name": "Liad Yosef",
    "url": "https://github.com/liady"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liady/es6-library-minimal/issues"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib",
    "dist"
  ],
  "homepage": "https://github.com/liady/es6-library-minimal",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.4.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.8",
    "del": "^3.0.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^8.0.0",
    "gulp-env": "^0.4.0",
    "gulp-eslint": "^5.0.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-util": "^3.0.6",
    "mocha": "^5.2.0",
    "mustache": "^2.3.0",
    "object-assign": "^4.0.1",
    "webpack": "^4.17.2",
    "webpack-node-externals": "^1.7.2"
  },
  "library": {
    "name": "kjs",
    "entry": "library.js",
    "dist-node": "kjs.js",
    "dist-web": "kjs.min.js",
    "bundle-node": true
  }
}
